    "build:packages": "yarn build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "lerna": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
