{
  "name": "lucas-utils",
  "version": "0.0.1",
  "author": "stardustrain <kthanterran@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stardustrain/lucas-utils.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "boot": "yarn --frozen-lockfile && yarn bootstrap",
